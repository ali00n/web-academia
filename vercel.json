{
    "buildCommand": "npm install && cd server && npx prisma generate",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/server.js"
        }
    ],
    "env": {
        "DATABASE_URL": "@database_url",
        "DIRECT_URL": "@direct_url",
        "JWT_SECRET": "@jwt_secret"
    }
}